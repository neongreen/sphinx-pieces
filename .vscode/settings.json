{
  "files.exclude": {
    "**/.ruff_cache/": true,
    "**/__pycache__/": true,
    "**/.pytest_cache/": true,
    "**/.mypy_cache/": true,
    "**/.venv/": true,
    "**/agent.egg-info/": true
  },
  "[toml]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[yaml]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[python]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "files.insertFinalNewline": true
}
